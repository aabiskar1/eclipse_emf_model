    <containmentFeature
        href="vending.ecore#//VendingMachine/transitions"/>
    <tool
        xsi:type="gmftool:CreationTool"
        href="vending.gmftool#//@palette/@tools.1/@tools.0"/>
    <diagramLink
        href="vending.gmfgraph#Transition"/>
    <sourceMetaFeature
        xsi:type="ecore:EReference"
        href="vending.ecore#//Transition/from"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="vending.ecore#//Transition/to"/>
  </links>
  <diagram>
    <diagramCanvas
        href="vending.gmfgraph#vendingMachine"/>
    <domainModel
        href="vending.ecore#/"/>
    <domainMetaElement
        href="vending.ecore#//VendingMachine"/>
    <palette
        href="vending.gmftool#//@palette"/>
