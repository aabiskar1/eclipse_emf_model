      </labelMappings>
      <tool
          xsi:type="gmftool:CreationTool"
          href="vending.gmftool#//@palette/@tools.0/@tools.0"/>
      <diagramNode
          href="vending.gmfgraph#Button"/>
    </ownedChild>
  </nodes>
  <nodes>
    <containmentFeature
        href="vending.ecore#//VendingMachine/events"/>
    <ownedChild>
      <domainMetaElement
          href="vending.ecore#//UserCoin"/>
      <labelMappings
          xsi:type="gmfmap:FeatureLabelMapping">
        <diagramLabel
            href="vending.gmfgraph#UserCoinLabel"/>
        <features
            href="vending.ecore#//Event/text"/>
      </labelMappings>
      <tool
          xsi:type="gmftool:CreationTool"
          href="vending.gmftool#//@palette/@tools.0/@tools.3"/>
      <diagramNode
          href="vending.gmfgraph#UserCoin"/>
    </ownedChild>
  </nodes>
  <links>
    <domainMetaElement
        href="vending.ecore#//Transition"/>
    <labelMappings
        xsi:type="gmfmap:FeatureLabelMapping">
      <diagramLabel
          href="vending.gmfgraph#TransitionLabel"/>
